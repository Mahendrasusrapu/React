/* index.js */
import React from "react";
import ReactDOM from "react-dom";
import { add,mul,sub,div} from "./calculator.jsx"

ReactDOM.render(
  <ul>
    <li>{add(1,2)}</li>
    <li>{mul(2,3)}</li>
    <li>{sub(7,2)}</li>
    <li>{div(5,2)}</li>
  </ul>,
  document.getElementById("root")
);

/* calculator.jsx */

function add(a,b) {
  return a+b; 
}
function mul(a,b){
  return a*b;
}
function sub(a,b){
  return a-b;
}
function div(a,b){
  return a/b;
}

export {add,mul,sub,div} ;
