/* Heading.jsx */

import React from 'react';

function Heading(){
  return <h1>My favourite Foods</h1>
}

export default Heading;

/* List.jsx*/

import React from "react";

function List(){
  return (
    <ul>
      <li>Apple</li>
      <li>Banana</li>
      <li>Orange</li>
    </ul>
  );
}

export default List;

/* App.jsx */

import React from "react";
import Heading from "./Heading.jsx";
import List from "./List.jsx";

function App(){
  return <div>
    <Heading />
    <List />
    </div>
}

export  default App;
