// understanding the components and props by using cards
import React from "react";
import ReactDOM from "react-dom";

function Card(props) {
  return (
    <div>
      <h2>{props.name}</h2>
      <img src={props.src} alt={props.alt} height="100px" />
      <p>{props.tel}</p>
      <p>{props.mail}</p>
    </div>
  );
}

ReactDOM.render(
  <div>
    <h1>My contacts</h1>
    <Card
      name="S.MAHENDRA SHARMA"
      src="data:image/jpeg;base64,"
      alt="unknown image"
      tel="+123 456 8900"
      mail="mahendra@gmail.com"
    />
    <Card
      name="Ravindra"
      src="data:image/jpeg;base64,"
      alt="unknown image"
      tel="+20202020202"
      mail="ravi@1023.ac.in"
    />
  </div>,
  document.getElementById("root")
);
